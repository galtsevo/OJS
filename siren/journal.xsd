                            <xs:element minOccurs="0" maxOccurs="1" name="fundings">
                              <xs:annotation>
                                <xs:documentation>
									Элемент, включающий в себя список элементов, значения которых содержат сведения о финансовой поддержке статьи
								</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
