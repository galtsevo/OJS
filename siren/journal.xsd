  <!-- article type dictionary -->
  <xs:simpleType name="typeArticleType">
    <xs:annotation>
      <xs:documentation xml:lang="ru">Типы статей</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="ABS">
        <xs:annotation>
          <xs:documentation xml:lang="ru">Аннотация</xs:documentation>
          <xs:documentation xml:lang="en">Abstract</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
