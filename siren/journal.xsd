  <!-- Unit Block -->
  <!-- file type -->
  <xs:simpleType name="typeIntegerOrEmpty">
    <xs:restriction base="xs:string">
      <xs:pattern value="[+-]?[0-9]+" />
      <xs:pattern value="" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="typeFile">
    <xs:annotation>
      <xs:documentation xml:lang="ru">Паттерн имени файла</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value=".+\.[\d\w]{2,4}" />
    </xs:restriction>
  </xs:simpleType>
  <!-- file description -->
  <xs:simpleType name="typeDesc">
    <xs:annotation>
      <xs:documentation xml:lang="ru">Описание прикрепляемых файлов</xs:documentation>
      <!--  <xs:documentation xml:lang="en">Attachment Description</xs:documentation> -->
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="fullText">
        <xs:annotation>
          <xs:documentation xml:lang="ru">Полный текст публикации в формате PDF</xs:documentation>
          <xs:documentation xml:lang="en">Full text of the publication in PDF</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="addition">
        <xs:annotation>
          <xs:documentation xml:lang="ru">Дополнительный материал(приложение) к публикации</xs:documentation>
          <xs:documentation xml:lang="en">Additional material to the publication</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="illustration">
        <xs:annotation>
          <xs:documentation xml:lang="ru">Иллюстрация к публикации (чаще всего в аннотации)</xs:documentation>
          <xs:documentation xml:lang="en">Illustration in the publication</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <!-- file description for Issue -->
  <xs:simpleType name="typeIssueDesc">
    <xs:annotation>
      <xs:documentation xml:lang="ru">Описание прикрепляемых файлов для выпуска журнала</xs:documentation>
      <!--  <xs:documentation xml:lang="en">Attachment Description</xs:documentation> -->
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="cover">
        <xs:annotation>
          <xs:documentation xml:lang="ru">Обложка в формате JPEG, JPG, PNG или GIF</xs:documentation>
          <xs:documentation xml:lang="en">Cover</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <!-- pdf file location description -->
  <xs:simpleType name="typeLocation">
    <xs:annotation>
      <xs:documentation xml:lang="ru">Описание места размещения pdf-файла</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="repository">
        <xs:annotation>
          <xs:documentation xml:lang="ru">Открытый архив или репозитарий</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="publisher">
        <xs:annotation>
          <xs:documentation xml:lang="ru">Сайт издательства</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <!-- pdf version description -->
  <xs:simpleType name="typeVersion">
    <xs:annotation>
      <xs:documentation xml:lang="ru">Описание версий pdf-файлов</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="submitted">
        <xs:annotation>
          <xs:documentation xml:lang="ru">Версия, отправленная в редакцию</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="accepted">
        <xs:annotation>
          <xs:documentation xml:lang="ru">Версия, принятая к публикации</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="published">
        <xs:annotation>
          <xs:documentation xml:lang="ru">Опубликованная версия</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="translated">
        <xs:annotation>
          <xs:documentation xml:lang="ru">Переводная версия</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
