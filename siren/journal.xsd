                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="B" type="xs:string" />
                                                              </xs:sequence>
                                                            </xs:complexType>
                                                          </xs:element>
                                                          <xs:element minOccurs="0" maxOccurs="unbounded" name="i">
                                                            <xs:complexType mixed="true">
                                                              <xs:sequence maxOccurs="unbounded">
                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="i" type="xs:string" />
                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="I" type="xs:string" />
                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="sup" type="xs:string" />
                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="SUP" type="xs:string" />
                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="sub" type="xs:string" />
                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="SUB" type="xs:string" />
                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="br" type="xs:string" />
                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="BR" type="xs:string" />
                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="b" type="xs:string" />
                                                                <xs:element minOccurs="0" maxOccurs="unbounded" name="B" type="xs:string" />
                                                              </xs:sequence>
                                                            </xs:complexType>
                                                          </xs:element>
