msgid "plugins.importexport.siren.displayName"
msgstr "Plugiciel d'exportation Siren XML"

msgid "plugins.importexport.siren.description"
msgstr "Exporter en format Siren XML les métadonnées de l'article pour indexation dans MEDLINE."

msgid "plugins.importexport.siren.export"
msgstr "Exporter des données"

msgid "plugins.importexport.siren.export.issues"
msgstr "Exporter des numéros"

msgid "plugins.importexport.siren.export.selectIssue"
msgstr "Sélectionner un numéro à exporter."

msgid "plugins.importexport.siren.export.articles"
msgstr "Exporter des articles"

msgid "plugins.importexport.siren.export.selectArticle"
msgstr "Sélectionner les articles à exporter."

msgid "plugins.importexport.siren.cliUsage"
msgstr ""
"Utilisation : \n"
"{$scriptName} {$pluginName} [xmlFileName] [journal_path] articles "
"[articleId1] [articleId2] ...\n"
"{$scriptName} {$pluginName} [xmlFileName] [journal_path] numéro [issueId]\n"

msgid "plugins.importexport.siren.cliError"
msgstr "ERREUR :"

msgid "plugins.importexport.siren.export.error.issueNotFound"
msgstr ""
"Aucun numéro ne correspond à l'identifiant du numéro spécifié « {$issueId} »."

msgid "plugins.importexport.siren.export.error.articleNotFound"
msgstr ""
"Aucun article ne correspond à l'identifiant de l'article spécifié « "
"{$articleId} »."
